<UserControl x:Class="zwg_china.client.RebateRecordsPage_TableRow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="TableStyleDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <StackPanel Width="750" Height="25" Orientation="Horizontal">
        <Grid Width="120">
            <TextBlock Name="text_Owner" Text="用户"/>
        </Grid>
        <Grid Width="120">
            <TextBlock Name="text_Source" Text="来源" />
        </Grid>
        <Grid Width="240">
            <TextBlock Name="text_GameName" Text="游戏" />
        </Grid>
        <Grid Width="120">
            <TextBlock Name="text_Sum" Text="金额"/>
        </Grid>
        <Grid Width="150">
            <TextBlock Name="text_CreatedTime" Text="时间"/>
        </Grid>
    </StackPanel>
</UserControl>
