<UserControl x:Class="zwg_china.backstage.control.AdministratorLandingRecordsPage_Table"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:loc="clr-namespace:zwg_china.backstage.control"
    mc:Ignorable="d">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="_CssOfTableRow.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <StackPanel Width="750" Height="30">
        <Grid Width="180">
            <TextBlock Name="text_Owner" Text="用户"/>
        </Grid>
        <Grid Width="210">
            <TextBlock Name="text_CreatedTime" Text="登陆时间" />
        </Grid>
        <Grid Width="180">
            <TextBlock Name="text_Ip" Text="登陆IP" />
        </Grid>
        <Grid Width="180">
            <TextBlock Name="text_Address" Text="登陆地址"/>
        </Grid>
    </StackPanel>
</UserControl>
