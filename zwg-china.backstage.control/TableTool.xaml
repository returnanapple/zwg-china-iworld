<UserControl xmlns:control="clr-namespace:zwg_china.backstage.control"  x:Class="zwg_china.backstage.control.TableTool"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Grid MinWidth="200" MinHeight="90">
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <!--外框-->
        <Canvas Grid.RowSpan="2" Name="bg"/>
        <!--标题-->
        <!--列表-->
        <!--分页选择区-->
        <StackPanel Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Right">
            
        </StackPanel>
    </Grid>
</UserControl>
