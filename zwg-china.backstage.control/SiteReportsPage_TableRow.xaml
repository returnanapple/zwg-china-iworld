<UserControl x:Class="zwg_china.backstage.control.SiteReportsPage_TableRow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:loc="clr-namespace:zwg_china.backstage.control"
    mc:Ignorable="d">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="_CssOfTableRow.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <StackPanel Width="750" Height="30">
        <Grid Width="130">
            <TextBlock Name="text_date" Text="日期"/>
        </Grid>
        <Grid Width="130">
            <TextBlock Name="text_CountOfSetUp" Text="注册人数" />
        </Grid>
        <Grid Width="130">
            <TextBlock Name="text_TimesOfLogin" Text="登陆次数" />
        </Grid>
        <Grid Width="130">
            <TextBlock Name="text_Recharge" Text="充值金额"/>
        </Grid>
        <Grid Width="130">
            <TextBlock Name="text_Withdrawal" Text="提现金额"/>
        </Grid>
        <StackPanel Width="100">
            <HyperlinkButton Content="详细信息" Click="ShowFullWindow" />
        </StackPanel>
    </StackPanel>
</UserControl>
