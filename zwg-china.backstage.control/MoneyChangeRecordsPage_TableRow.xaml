<UserControl x:Class="zwg_china.backstage.control.MoneyChangeRecordsPage_TableRow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:loc="clr-namespace:zwg_china.backstage.control"
    mc:Ignorable="d">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="_CssOfTableRow.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <StackPanel Width="750" Height="30">
        <Grid Width="100">
            <TextBlock Name="text_Owner" Text="用户"/>
        </Grid>
        <Grid Width="100">
            <TextBlock Name="text_Type" Text="类型" />
        </Grid>
        <Grid Width="100">
            <TextBlock Name="text_Income" Text="收入"/>
        </Grid>
        <Grid Width="100">
            <TextBlock Name="text_Expenses" Text="支出"/>
        </Grid>
        <Grid Width="100">
            <TextBlock Name="text_Sum" Text="变动"/>
        </Grid>
        <Grid Width="100">
            <TextBlock Name="text_Money" Text="余额"/>
        </Grid>
        <Grid Width="150">
            <TextBlock Name="text_CreatedTime" Text="时间"/>
        </Grid>
    </StackPanel>
</UserControl>
