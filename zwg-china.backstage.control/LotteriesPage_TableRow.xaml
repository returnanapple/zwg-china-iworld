<UserControl x:Class="zwg_china.backstage.control.LotteriesPage_TableRow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:loc="clr-namespace:zwg_china.backstage.control"
    mc:Ignorable="d">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="_CssOfTableRow.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <StackPanel Width="750" Height="30">
        <Grid Width="120">
            <TextBlock Name="text_Ticket" Text="彩票"/>
        </Grid>
        <Grid Width="130">
            <TextBlock Name="text_Issue" Text="期号" />
        </Grid>
        <Grid Width="230">
            <TextBlock Name="text_Values" Text="开奖号码" />
        </Grid>
        <Grid Width="120">
            <TextBlock Name="text_Sources" Text="来源"/>
        </Grid>
        <Grid Width="150">
            <TextBlock Name="text_CreatedTime" Text="时间" />
        </Grid>
    </StackPanel>
</UserControl>
